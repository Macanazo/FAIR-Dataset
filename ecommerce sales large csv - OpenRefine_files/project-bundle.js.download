	    		    self._dismissBusy();
	    		    RdfReconciliationManager.registerService(data,self._level);
			    }
		    });
		});

	}).appendTo(footer);
	
	$('<button></button>').addClass('button').text($.i18n('rdf-ext-buttons/cancel')).click(function() {
