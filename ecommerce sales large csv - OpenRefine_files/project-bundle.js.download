        config.regex = true;
      } else if (mode === "number") {
        if(elmts.reverseTransitionNumbers[0].checked) {
          config.separator = "(?<=\\p{L}|[\\p{L}][\\s])(?=\\p{Digit})";
        } else {
          config.separator = "(?<=\\p{Digit}|[\\p{Digit}][\\s])(?=\\p{L})";
        }
        config.regex = true;
      }

      Refine.postCoreProcess(
        "split-multi-value-cells",
        config,
        null,
        { rowsChanged: true },
        { onDone: dismiss }
      );
    });
  };

